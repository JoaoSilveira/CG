<Application x:Class="CG_Final.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:CG_Final"
             xmlns:conv="clr-namespace:CG_Final.Util"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
         <conv:TitleConverter x:Key="TitleConverter"/>
         <conv:VerticesConverter x:Key="VerticesConverter"/>
        <conv:DecimalConverter x:Key="DecimalConverter"/>
        <conv:PerspectiveToBoolConverter x:Key="PerspectiveToBoolConverter"/>
        <conv:SelectedCameraConverter x:Key="SelectedCameraConverter"/>
    </Application.Resources>
</Application>
